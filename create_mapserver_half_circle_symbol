## R

### prepare "circle data"
radius <- 1
theta <- seq(0, 2 * pi, length = 200)

x<-radius * cos(theta)
y<- radius * sin(theta)

X<-c(x[1:50],x[151:200])
Y<-c(y[1:50],y[151:200])


write.csv2(data.frame(X+0.5,Y+0.5),file='half_circle.csv',col.names=F,row.names=F,sep=" ")

## mapserver

### create symolset
SYMBOLSET
    SYMBOL
        NAME "circle"
        TYPE ellipse
        POINTS
            1 1
        END
        FILLED true
        #ANCHORPOINT 0.5 0.5
    END
