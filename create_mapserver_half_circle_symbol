        CLASS
            NAME 'évek: < 1950' 
            EXPRESSION ([selected] = 1 and [datum] < 1951 and '[datum]' ~ "") # selected / highlighted
            STYLE
                SYMBOL "circle" 
                SIZE 5
                OPACITY 100
                COLOR 255 255 255
                OUTLINECOLOR 30 30 30
                WIDTH 0.2
            END
        END
        CLASS
            NAME 'évek: 1951 - 1990' 
            EXPRESSION ([selected] = 1 and [datum] <= 1990 and [datum] >= 1951) # selected / highlighted
